<h1>Listing team_competitors</h1>

<table>
  <tr>
    <th>Team</th>
    <th>Competitor</th>
    <th>Position</th>
    <th>Card</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @team_competitors.each do |team_competitor| %>
  <tr>
    <td><%= team_competitor.team_id %></td>
    <td><%= team_competitor.competitor_id %></td>
    <td><%= team_competitor.position %></td>
    <td><%= team_competitor.card_id %></td>
    <td><%= link_to 'Show', team_competitor %></td>
    <td><%= link_to 'Edit', edit_team_competitor_path(team_competitor) %></td>
    <td><%= link_to 'Destroy', team_competitor, confirm: 'Are you sure?', method: :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Team competitor', new_team_competitor_path %>
